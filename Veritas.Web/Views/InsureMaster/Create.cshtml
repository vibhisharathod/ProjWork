@section Myscripts {
    <!-- page specific plugin styles -->

    <link rel="stylesheet" href="~/assets/css/jquery-ui.css" />
    <link href="~/assets/css/magicsuggest.css" rel="stylesheet" />
    <script src="~/assets/js/jquery-ui.js"></script>
    <script src="~/assets/js/jquery.ui.touch-punch.js"></script>
    <script src="~/assets/js/magicsuggest.js"></script>

    <script type="text/javascript">
        //Loading Policy Data
        var statusData = Common_AjaxGET("/LookUpTables/PolicyStatus");
        if (statusData.length > 0) {

            var _elmTxtPolicy = $('#ddSelectStatus').magicSuggest({
                allowFreeEntries: false,
                maxSelection: 1,
                valueField: 'Value',
                displayField: 'Text',
                data: statusData
            });
            $(_elmTxtPolicy).on('selectionchange', function () {
                _arrPolicy = this.getValue();
            });
        }
        var _arrPolicy = new Array();
        //Loading Business Data
        var businessstatusData = Common_AjaxGET("/LookUpTables/BusinessStatus");
        if (businessstatusData.length > 0) {

            var _elmTxtbusiness = $('#ddSelectBusinessStatus').magicSuggest({
                allowFreeEntries: false,
                maxSelection: 1,
                valueField: 'Value',
