        }


        var _arrCompanies = new Array();
        var _arrProductTypes = new Array();
        var _arrSE = new Array();
        var _arrMA = new Array();
        var _arrAgency = new Array();
        var _arrSA = new Array();


        function DiffAmt() {
            var id1 = document.getElementById("ChequeAmt");
            var id2 = document.getElementById("CashAmt");
            var id3 = document.getElementById("Return_Amt");
            id3.value = Math.round(parseFloat(id1.value) - parseFloat(id2.value));
        }

        function myFunc() {
            var BasicPremium = document.getElementById("BasicPremium");
            var LoadingAmount = document.getElementById("LoadingAmt");
            var TPPremium = document.getElementById("ThirdPartyPrem");
            var PAtoUNPass = document.getElementById("PAtoUnnamed");
            var PAtoOwnerDriver = document.getElementById("PAtoCarOwner");
            var PrmAftLoad = document.getElementById("PremiumAfterLoading");


            if ((BasicPremium.value == "")) {
                BasicPremium.value = 0;
            }
            if ((LoadingAmount.value == "")) {
                LoadingAmount.value = 0;
            }
            if ((TPPremium.value == "")) {
                TPPremium.value = 0;
            }
            if ((PAtoUNPass.value == "")) {
                PAtoUNPass.value = 0;
            }
            if ((PAtoOwnerDriver.value == "")) {
                PAtoOwnerDriver.value = 0;
